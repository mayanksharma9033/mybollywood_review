<h1>All Movies</h1>
<hr/>

<% @movies.each do |movie| %>
<div class="row">
	<div class="col-md-8 col-md-offset-2 well">
		<h2><%=link_to movie.movie_name, movie_path(movie) %></h2>
		<p><%=movie.description %></p>
		<p><%=movie.movie_length %></p>
		<p><%=movie.director_name %></p>
		<p><%=movie.rating %></p>
	</div>
</div>
<%end%>




